{
  "name": "video-processing-render",
  "version": "1.0.0",
  "main": "src/server.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "prisma generate --schema=./prisma/schema.prisma && node src/server.js",
    "build": "prisma generate --schema=./prisma/schema.prisma",
    "dev": "prisma generate --schema=./prisma/schema.prisma && nodemon src/server.js",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "start:transcription": "prisma generate --schema=./prisma/schema.prisma && node src/workers/transcription.js",
    "start:summary": "prisma generate --schema=./prisma/schema.prisma && node src/workers/summary.js",
    "start:article": "prisma generate --schema=./prisma/schema.prisma && node src/workers/article.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Video processing application with Render and Cloudflare R2",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@prisma/client": "^5.10.2",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.10",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "prisma": "^5.10.2",
    "redis": "^4.7.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
